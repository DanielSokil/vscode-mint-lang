{
    "name": "mint-lang",
    "displayName": "Mint Lang",
    "description": "A refreshing language for the front-end web.",
    "version": "0.0.3",
    "publisher": "faustinoaq",
    "engines": {
        "vscode": "^1.21.0"
    },
    "icon": "images/icon.png",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/faustinoaq/vscode-mint-lang"
    },
    "bugs": "https://github.com/faustinoaq/vscode-mint-lang/issues",
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "mint",
        "syntax",
        "languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "mint",
                "aliases": [
                    "Mint",
                    "mint"
                ],
                "extensions": [
                    ".mint"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "mint",
                "scopeName": "source.mint",
                "path": "./syntaxes/mint.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "mint",
                "path": "./snippets/mint.json"
            }
        ]
    }
}